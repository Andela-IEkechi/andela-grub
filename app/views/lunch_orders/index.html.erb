<h1 class="page-header">My Lunch Orders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lunch_orders.each do |lunch_order| %>
      <tr>
        <td><%= lunch_order.menu_content.food_item %> |</td>
        <td><%= link_to 'Edit', edit_lunch_order_path(lunch_order) %> |</td>
        <td><%= link_to 'Destroy', lunch_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lunch order', new_lunch_order_path %>
